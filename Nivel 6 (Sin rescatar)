class Player
def play_turn(warrior)
    if warrior.feel.empty?
      if warrior.health<20 && (warrior.health >= @health) 
        warrior.rest!
      elsif warrior.health<10 && (warrior.health < @health)
      warrior.walk!:backward
      elsif warrior.walk!
      end
    else
      if warrior.feel.captive?
        warrior.rescue!
      else
      warrior.attack!
      end
    end
  @health=warrior.health
  end
end
